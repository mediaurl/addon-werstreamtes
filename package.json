{
  "name": "mediaurl-example",
  "version": "0.0.0",
  "main": "dist",
  "scripts": {
    "build": "tsc",
    "start": "node .",
    "develop": "ts-node-dev --transpile-only --no-notify src",
    "test": "jest test/",
    "prepare": "npx simple-git-hooks"
  },
  "dependencies": {
    "@mediaurl/sdk": "^2.0.0",
    "cheerio": "^1.0.0-rc.10",
    "node-fetch": "^2.6.1",
    "puppeteer-core": "^10.0.0",
    "puppeteer-extra": "^3.1.18",
    "puppeteer-extra-plugin-block-resources": "^2.2.9",
    "puppeteer-extra-plugin-stealth": "^2.7.8",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.29",
    "@types/node": "^14.14.28",
    "@types/node-fetch": "^2.5.10",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.1",
    "simple-git-hooks": "^2.4.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
